# Yandex Cloud API ключи (ОБЯЗАТЕЛЬНО заполнить!)
YANDEX_API_KEY=your_yandex_api_key_here
YANDEX_FOLDER_ID=your_yandex_folder_id_here

# Модели Yandex (можно оставить по умолчанию)
YANDEX_LLM_MODEL=yandexgpt-lite
YANDEX_EMBEDDING_MODEL=text-search-doc

# Размерность векторов (по умолчанию для Yandex GPT)
VECTOR_DIMENSION=256

# PostgreSQL настройки (можно оставить по умолчанию)
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=rag_db
POSTGRES_USER=rag_user
POSTGRES_PASSWORD=rag_password

# OpenSearch настройки (можно оставить по умолчанию)
OPENSEARCH_HOST=opensearch
OPENSEARCH_PORT=9200
OPENSEARCH_USER=admin
OPENSEARCH_PASSWORD=admin

# MinIO настройки (можно оставить по умолчанию)
MINIO_ENDPOINT=minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=rag-documents
MINIO_SECURE=false 